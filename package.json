{
	"name": "dot-wasm",
	"version": "0.0.0",
	"main": "index.js",
	"author": "Niklas Mischkulnig",
	"license": "MIT",
	"scripts": {
		"build": "make",
		"demo-browser": "parcel build demo-browser --no-source-maps",
		"demo-browser-deploy": "now dist --name demo-browser/dot-wasm --public",
		"demo-node": "node demo-node.js",
		"demo-native": "clang dot-wasm-native.c -I/usr/local/opt/graphviz/include -L/usr/local/opt/graphviz/lib -L/usr/local/opt/graphviz/lib/graphviz -lgvpr -lgvc -lcgraph -lgvplugin_core -lgvplugin_dot_layout -o dot-wasm-native && ./dot-wasm-native"
	},
	"devDependencies": {
		"parcel": "^2.0.0-nightly.141"
	},
	"browser": {
		"fs": false,
		"path": false,
		"process": false
	}
}
